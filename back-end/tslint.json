{
    "rules": {
        "max-line-length": {
            "options": [120]
        },
        "semicolon": true,
        "new-parens": true,
        "no-arg": true,
        "no-bitwise": true,
        "no-conditional-assignment": true,
        "no-consecutive-blank-lines": false,
        "whitespace": true,
        "triple-equals": true,
        "prefer-const": true,
        "class-name": true
    },
    "jsRules": {
        "max-line-length": {
            "options": [120]
        }
    }
  }